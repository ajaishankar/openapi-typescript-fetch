{
  "name": "openapi-typescript-fetch",
  "description": "A typed fetch client for openapi-typescript",
  "version": "1.1.3",
  "engines": {
    "node": ">= 12.0.0",
    "npm": ">= 7.0.0"
  },
  "author": "Ajai Shankar",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "browser": "./dist/esm/index.js",
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ajaishankar/openapi-typescript-fetch"
  },
  "keywords": [
    "fetch",
    "client",
    "swagger",
    "typescript",
    "ts",
    "openapi",
    "openapi 3",
    "node"
  ],
  "bugs": {
    "url": "https://github.com/ajaishankar/openapi-typescript-fetch/issues"
  },
  "homepage": "https://github.com/ajaishankar/openapi-typescript-fetch#readme",
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.31.0",
    "codecov": "^3.8.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "msw": "^0.35.0",
    "npm-run-all2": "^6.0.2",
    "prettier": "^2.4.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^27.0.0",
    "ts-node": "^10.0.0",
    "type-coverage": "^2.22.0",
    "typescript": "~4.6.0",
    "whatwg-fetch": "^3.6.2"
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true,
    "semi": false
  },
  "scripts": {
    "build:0-clean": "run-s clean",
    "build:1-esm": "tsc --project tsconfig.json",
    "build:2-cjs": "tsc --project tsconfig.cjs.json",
    "build": "run-s build:*",
    "clean": "rimraf './dist'",
    "lint:eslint": "eslint .",
    "lint:type-coverage": "type-coverage --strict --at-least 95 --ignore-files='test/**' --ignore-files='dist/**'",
    "lint:typecheck-tests": "tsc --noEmit --project test/tsconfig.json",
    "lint:typecheck": "tsc --noEmit --project tsconfig.json",
    "lint": "run-p lint:*",
    "prepare": "npm run build",
    "test:coverage:local": "npm run build && jest --no-cache --collectCoverage",
    "test:coverage": "npm run build && jest --no-cache --coverage && codecov",
    "test": "npm run build && jest"
  }
}
